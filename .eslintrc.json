{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:react/recommended", "standard", "plugin:prettier/recommended"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "warn",
        "no-unused-vars": "warn",
        "no-undef": "warn",
        "no-underscore-dangle": "off",
        "react/react-in-jsx-scope": "off",
        "react/jsx-no-undef": "warn"
    },
    "plugins": ["react", "prettier"]
}
